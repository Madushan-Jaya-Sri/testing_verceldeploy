{
    "functions": {
      "api/process": {
        "handler": "app.process_pdf",
        "runtime": "python3.8"
      },
      "api/ask": {
        "handler": "app.ask_question",
        "runtime": "python3.8"
      }
    },
    "routes": [
      { "src": "/process", "dest": "api/process" },
      { "src": "/ask", "dest": "api/ask" },
      { "src": "/(.*)", "dest": "app.py" }
    ]
  }
  